{
  "name": "gemini-search",
  "version": "1.2.0",
  "description": "使用 Gemini API 进行增强的网络搜索和网页内容分析",
  "type": "search",
  "entryPoint": "index.js",
  "author": "iFlow CLI",
  "license": "MIT",
  "keywords": ["gemini", "search", "google", "web-search", "openai", "fetch"],
  "dependencies": {},
  "environment": {
    "requiredEnvVars": ["GEMINI_BASE_URL", "GEMINI_API_KEY"],
    "optionalEnvVars": ["GEMINI_MODEL"],
    "description": {
      "GEMINI_BASE_URL": "OpenAI 兼容的 API 基础 URL，例如 https://generativelanguage.googleapis.com/v1beta/openai",
      "GEMINI_API_KEY": "Gemini API 密钥，从 https://aistudio.google.com/ 获取",
      "GEMINI_MODEL": "默认使用的模型名称，默认为 gemini-2.5-flash-lite"
    }
  },
  "commands": [
    {
      "name": "search",
      "description": "使用 Google 搜索网络内容",
      "parameters": {
        "query": {
          "type": "string",
          "required": true,
          "description": "搜索查询",
          "maxLength": 1000
        },
        "numResults": {
          "type": "number",
          "required": false,
          "default": 10,
          "min": 1,
          "max": 100,
          "description": "返回结果数量"
        },
        "timeRange": {
          "type": "string",
          "required": false,
          "description": "时间范围过滤"
        },
        "json": {
          "type": "boolean",
          "required": false,
          "default": true,
          "description": "是否返回结构化 JSON 结果"
        },
        "model": {
          "type": "string",
          "required": false,
          "description": "覆盖默认模型"
        }
      }
    },
    {
      "name": "fetch",
      "description": "获取并分析网页内容",
      "parameters": {
        "url": {
          "type": "string",
          "required": true,
          "description": "目标网页 URL"
        },
        "prompt": {
          "type": "string",
          "required": false,
          "default": "",
          "maxLength": 2000,
          "description": "分析提示词"
        },
        "model": {
          "type": "string",
          "required": false,
          "description": "覆盖默认模型"
        }
      }
    }
  ],
  "model": {
    "name": "gemini-2.5-flash-lite",
    "provider": "google"
  },
  "timeout": 30000,
  "documentation": {
    "readme": "README.md",
    "example": "example.js"
  }
}
